{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "e6f42813-207d-4786-8152-44161962362f",
            "flow": "xtls-rprx-vision",
            "email": "admin@itstudylab.us"
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": "nginx-decoy:8080",
            "xver": 0
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "certificates": [
            {
              "certificateFile": "/etc/letsencrypt/live/itstudylab.us/fullchain.pem",
              "keyFile": "/etc/letsencrypt/live/itstudylab.us/privkey.pem"
            }
          ]
        },
        "tcpSettings": {
            "header": { "type": "none" }
        },
        "sockopt": {
            "tcpNoDelay": true
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom"
    }
  ],
  "policy": {
    "levels": {
      "0": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 2,
        "downlinkOnly": 5,
        "statsUserUplink": false,
        "statsUserDownlink": false,
        "bufferSize": 4096
      }
    }
  }
}

